.then(res => {\r\n\t\t\t\t\tif (res.data.code == 0) {\r\n\t\t\t\t\t\tthis.myFollows = res.data.data;\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t}\r\n\t},\r\n};\r\n</script>\r\n\r\n<style>\r\n#my-follows .delete-button {\r\n    height: 100%;\r\n    background-color: red;\r\n    border: 0;\r\n    color: #fff;\r\n  }\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/mine"}]}